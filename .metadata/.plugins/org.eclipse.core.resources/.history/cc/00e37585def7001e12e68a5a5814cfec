package chapter02;

public class AssignTest04 {

	public static void main(String[] args) {
		
		System.out.println("==전위연산==");
		int gameScore = 150;
		int lastScore = ++gameScore;
		System.out.println(gameScore);
		System.out.println(lastScore);
		
		System.out.println("============");
		lastScore = --gameScore;
		// *** 중요: 먼저 1 뺀 걸 gameScore에 저장 한다 
		System.out.println(gameScore);
		System.out.println(lastScore);

		
	}

}
