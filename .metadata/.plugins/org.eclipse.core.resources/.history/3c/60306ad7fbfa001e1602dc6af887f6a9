package chapter03.SWITCH;

public class SwitchCase01 {

	public static void main(String[] args) {
		
		int ranking = 2;
		char medalColor;
		
		// swi + ctrl +space -> 자동완성 
		
		switch (ranking) {
		case 1:
			medalColor='금';
			break;
		case 2:
			medalColor='은';
			break;

		default:
			medalColor='노';
			break;
		}

	}

}
